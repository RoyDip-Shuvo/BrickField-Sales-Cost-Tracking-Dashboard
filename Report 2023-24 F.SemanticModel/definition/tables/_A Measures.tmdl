	measure 'Total Expence' = SUM(AMB_Bricks_Expance_Sheet[Amount])
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: 39ce5621-388f-4414-83a9-251ea160ef06

	measure Coal_Expence = CALCULATE([Total Expence], AMB_Bricks_Expance_Sheet[Sub_Head]="Coal")
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: 208f025b-98cf-462a-87bb-07acca3d4d79

	measure Sardar_Expence = CALCULATE([Total Expence], AMB_Bricks_Expance_Sheet[Acc_Head]= "SD_Payment")
		formatString: #,0
		lineageTag: 6b9b2b72-8a6d-4521-95c1-3413fdbe59f7

	measure 'Offical Cost' = CALCULATE([Total Expence], AMB_Bricks_Expance_Sheet[Acc_Head]="Offical Cost")
		formatString: #,0
		lineageTag: c7bdc014-0775-4076-8da2-4d9d6730494e

	measure Total_Sales = SUM(Vata_Sales[Final Price])
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: 91abb979-9473-4e75-8fee-2e3c819f2de2

	measure Profit = [Total_Sales]- [Total Expence]
		lineageTag: 7bce886d-6d1c-47d6-8c40-92cb624666f3

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Total_Production = SUM(Unload_table[Value])
		formatString: #,0
		lineageTag: 1b1644ee-0836-48cd-a8ac-1143aa581f35

	measure 'Unload %' =
			
			Var unload = [Total_Production]
			Var Total_unload = CALCULATE([Total_Production], ALLSELECTED(Unload_table[Attribute]))
			VAR Result = DIVIDE(unload, Total_unload,0)
			RETURN
			Result
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: f0c905c7-6847-4709-94b7-ea464e236178

	measure Xcavator_Expence = CALCULATE([Total Expence], AMB_Bricks_Expance_Sheet[Acc_Head]="Xcavator")
		formatString: 0
		lineageTag: 75b6336d-72e5-423d-9eca-1b7f6e8fd7ca

	measure 'Sales %' =
			
			DIVIDE([Total_Sales],
			    CALCULATE([Total_Sales], All(Vata_Sales)),0)
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: 493f2e8b-fd50-4996-ae3f-72692dc960bd

	measure 'Unit Sold' = SUM(Vata_Sales[Actual Quantity])
		lineageTag: 4894f485-d141-4a3e-9d26-a3ff5c5d97df

