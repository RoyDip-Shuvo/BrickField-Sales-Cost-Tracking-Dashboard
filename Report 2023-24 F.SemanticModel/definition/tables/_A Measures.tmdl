	measure 'Previous Year YTD' =
			
			SWITCH(
			    TRUE(),
			    SELECTEDVALUE(Parameter[Parameter Fields]) = "Expense", [PY Expence YTD],
			    SELECTEDVALUE(Parameter[Parameter Fields]) = "Sales", [PY Sales YTD],
			    SELECTEDVALUE(Parameter[Parameter Fields]) = "Production", [PY Production YTD],
			    BLANK()
			)
		lineageTag: 70edc807-fcc5-4fc5-8765-aec0e155f93d

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'PY Sales YTD' = ```
			
			CALCULATE(
